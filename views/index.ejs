<!DOCTYPE html>  
<html lang="ko">  
<head>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>  
    <meta charset="UTF-8">  
    <meta name="viewport" content="width=device-width, initial-scale=1.0">  
    <title>My Page</title>  
    <link rel="stylesheet" href="/css/style.css">  
</head>  
<body>  
    <!-- 네비게이션 바 -->  
    <header class="main-header">  
        <nav class="navbar">  
            <div class="navbar-logo">  
                <a href="#home">About Me</a>  
            </div>  
            <ul class="nav-links">  
                <li><a href="#home">홈</a></li>  
                <li><a href="#profile">프로필</a></li>  
                <!-- <li class="nav-divider">|</li>   -->
                <li><a href="#career">경력</a></li>  
                <li><a href="#contact">문의하기</a></li>  
                <li><a href="#contact-list">문의목록</a></li>  
            </ul>  
        </nav>  
    </header>  

    <!-- 홈 섹션 -->  
    <section id="home" class="section">  
        <div class="home-content">  
            <div class="home-text">  
                <h1>Let me introduce<br><span>myself</span></h1>  
                <p>We Add Value to Your House<br>We will put in precious value and happiness in design</p>  
            </div>  
            <div class="slider-container">  
                <div class="slider">  
                    <img src="8.jpg" alt="Slide 1" class="slide active">  
                    <img src="3.jpeg" alt="Slide 2" class="slide">  
                </div>  
                <div class="slider-dots">  
                    <span class="dot active"></span>  
                    <span class="dot"></span>  
                </div>  
            </div>  
        </div>  
    </section>  

    <!-- 프로필 섹션 -->  
    <section id="profile" class="section">  
        <div class="section-content">  
            <%- include('profile') %>  
        </div>  
    </section>    

    <!-- 블로그 섹션 -->  
    <section id="career" class="section">  
        <div class="section-content">  
            <%- include('career') %> 
        </div>  
    </section>  

    <!-- 문의하기 섹션 -->  
    <section id="contact" class="section">  
        <div class="section-content">  
            <%- include('contact') %> 
        </div>  
    </section>  

    <!-- 문의목록 섹션 -->  
    <section id="contact-list" class="section">  
        <div class="section-content">  
            <%- include('contactList') %>  
        </div>  
    </section>  

    <script src="/js/script.js"></script>  
</body>  
</html>